# Express server
PORT=3000

# Path to the Nginx Proxy Manager SQLite database (e.g., /data/app.db)
NPM_SQLITE_PATH=/data/app.db

# Publicly exposed ports (for reference/display)
PUBLIC_HTTP_PORT=80
PUBLIC_HTTPS_PORT=443

# Basic Authentication credentials for the UI and API
BASIC_AUTH_USERNAME=admin
BASIC_AUTH_PASSWORD=admin

# Poll interval for refreshing data from SQLite (milliseconds)
POLL_INTERVAL_MS=60000

# Page title for the portal UI
PAGE_TITLE=NPM Proxy Portal
